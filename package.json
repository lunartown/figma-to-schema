{
  "name": "figma-to-json-schema",
  "version": "0.1.0",
  "description": "Figma plugin for bidirectional conversion between API documentation tables and JSON Schema/OpenAPI",
  "main": "dist/code.js",
  "scripts": {
    "build": "webpack --mode=production",
    "dev": "webpack --mode=development --watch",
    "lint": "eslint src --ext .ts,.tsx",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "figma",
    "plugin",
    "json-schema",
    "openapi",
    "api-documentation"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.28.4",
    "@babel/preset-env": "^7.28.3",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@figma/plugin-typings": "^1.90.0",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "babel-loader": "^10.0.0",
    "css-loader": "^6.8.1",
    "eslint": "^8.56.0",
    "html-inline-script-webpack-plugin": "^3.2.1",
    "html-webpack-plugin": "^5.6.0",
    "style-loader": "^3.3.3",
    "ts-loader": "^9.5.1",
    "typescript": "^5.3.3",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "ajv": "^8.12.0",
    "js-yaml": "^4.1.0",
    "openapi-types": "^12.1.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
